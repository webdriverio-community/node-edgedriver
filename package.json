{
  "name": "edgedriver",
  "version": "5.3.9",
  "description": "Microsofts' EdgeDriver for Node.js",
  "homepage": "https://webdriver.io",
  "license": "MIT",
  "author": {
    "name": "Barrett Sonntag",
    "email": "barrett@sosuke.com",
    "url": "https://www.sosuke.com"
  },
  "keywords": [
    "edgedriver",
    "edgedriverserver",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/node-edgedriver.git"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": [
      {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./dist/cjs/index.js"
    ]
  },
  "bin": {
    "edgedriver": "./bin/edgedriver.js"
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "prepare": "husky install",
    "install": "test -f ./dist/install.js && node ./dist/install.js || echo \"Skipping install, project not build!\"",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "test:unit": "vitest --run",
    "test:e2e": "node --loader ts-node/esm ./tests/test.e2e.ts",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/unzipper": "^0.10.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "release-it": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "wait-port": "^1.1.0",
    "webdriverio": "^8.27.0"
  },
  "dependencies": {
    "@wdio/logger": "^8.24.12",
    "decamelize": "^6.0.0",
    "edge-paths": "^3.0.5",
    "node-fetch": "^3.3.2",
    "unzipper": "^0.10.14",
    "which": "^4.0.0"
  }
}
